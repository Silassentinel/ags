{
  "name": "getting-started",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "node scripts/preserve-build.js",
    "build:standard": "astro build --config astro.config.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "test": "npx jest --no-cache --detectOpenHandles --verbose",
    "test:tsx": "npx jest --no-cache --detectOpenHandles --verbose --testMatch=\"**/test/**/*.test.tsx\"",
    "test:ts": "npx jest --no-cache --detectOpenHandles --verbose --testMatch=\"**/test/ts/**/*.test.ts\"",
    "test:all": "npx jest --no-cache --detectOpenHandles --verbose",
    "test:ci": "npx jest --no-cache --detectOpenHandles --ci",
    "test:report": "npx jest --no-cache --detectOpenHandles --verbose --json --outputFile=test-results.json || true",
    "test:report:html": "npx jest --no-cache --detectOpenHandles --reporters=default --reporters=jest-html-reporter || true",
    "test:json": "npx jest --no-cache --detectOpenHandles --json > test-results.json || true",
    "test:html": "npx jest --no-cache --detectOpenHandles"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/preact": "^4.0.11",
    "@astrojs/rss": "^4.0.11",
    "@preact/preset-vite": "^2.10.1",
    "astro": "^5.7.10",
    "astro-compress": "^2.3.8",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.7.0",
    "preact": "^10.17.1",
    "typescript": "^5.2.2",
    "vite": "^6.3.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/preact-hooks": "^1.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2"
  }
}
