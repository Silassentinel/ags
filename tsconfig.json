  "extends": "astro/tsconfigs/strict",
  "include": ["src", ".astro/types.d.ts"],
  "exclude": ["src/components/Main/*"],
  "strict": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true
